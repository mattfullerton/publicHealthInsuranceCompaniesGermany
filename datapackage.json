{
  "name": "publicHealthInsuranceCompaniesGermany",
  "title": "Public Health Insurances in Germany",
  "description": "Name, States served, Internet address, Postal Address and Email Address of public health insurances in Germany. Data is sourced from the GKV-Spitzenverband and turned into a standard normalized CSV.",
  "licenses": [
    {
      "id": "odc-pddl",
      "url": "http://opendatacommons.org/licenses/pddl/"
    }
  ],
  "keywords": [ "health", "Germany", "contact"],
  "version": "2012",
  "last_updated": "2013-09-26",
  "sources": [{
    "name": "GKV-Spitzenverband",
    "web": "http://www.gkv-spitzenverband.de/krankenversicherung/krankenversicherung_grundprinzipien/alle_gesetzlichen_krankenkassen/alle_gesetzlichen_krankenkassen.jsp"
  }],
  "resources": [
    {
      "path": "data/GermanPublicHealthInsurances.csv",
      "schema": {
        "fields": [
          {
            "id": "name",
            "description": "Insurance Company Name",
            "type": "string"
          },
          {
            "id": "webaddress",
            "description": "Web address, without http://"
            "type": "string"
          },
          {
            "id": "states",
            "description": "State(s) served or employee only",
            "type": "string"
          },
          {
            "id": "email",
            "description": "Email address or contact form link", 
            "type": "string"
          },
          {
            "id": "address",
            "description": "Street Address", 
            "type": "string"
          },
          {
            "id": "phone",
            "description": "Telephone number (International or from within Germany)", 
            "type": "string"
          }
        ]
      }
    }
  ]
}
